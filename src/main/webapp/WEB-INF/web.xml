<?xml version="1.0" encoding="UTF-8" ?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
	<env-entry>
		<env-entry-name>dbConn</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>jdbc:mysql://localhost:3306/swbd?useUnicode=true&amp;useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=Europe/Rome</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>dbUser</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>root</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>dbPass</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value></env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>tokenDuration</env-entry-name>
		<env-entry-type>java.lang.Integer</env-entry-type>
		<env-entry-value>14400</env-entry-value> <!-- 4 ore -->
	</env-entry>
	<env-entry>
		<env-entry-name>JWTSecret</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>secret</env-entry-value>
	</env-entry>
</web-app>